#include <bits/stdc++.h>
using namespace std;
using ll =long long;
const int N = 0;



int main(){
    
    int t;cin>>t;
    while(t--){
        
        ll a,b;
        char op;
        cin>>a>>op;
        while(op != '='){
            cin>>b;
            if(op == '+'){a = a+b;}
            else if(op == '-'){a = a-b;}
            else if(op == '/'){a = a/b;}
            else if(op == '*'){a = a*b;}
            cin>>op;
        }
        cout<<a<<"\n";
        
    }      
}
